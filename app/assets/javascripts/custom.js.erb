$(document).on('mousedown','a[data-confirm]',function(e){
  e.preventDefault();
  element = this;
  link = $(e.target);
  message = link.data('confirm');
  swal(message, {buttons: ["Cancel!", "Ok!"], icon: "warning"})
  .then(function(val){
    if(val == true)
    {
      element.removeAttribute("data-confirm");
      element.click();
    }
  });
});
